 #include<stdio.h>
 //求素数
int main()
{
	int a = 0; int b = 0; int cont = 0; int c = 0; int d = 0;
	for (a = 100; a <= 200; a++)
	{
		for (b = 2; b < a; b++)
		{
			if (a % b == 0)
			{
				break;
			}
		}
			if(a==b)
			{
				printf("%d\n", a);
				cont++;
				c = c + a;
			
			}
	
		
	}
	printf("%d\n", cont);
	printf("%d\n", c);
	system("pause");
	return 0;
}
