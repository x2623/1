#include<stdio.h>
#include<stdlib.h>
//求公约数
int main()
{
	int a = 24;
	int b = 40;
	int c, d, e, f, g;
	int h = 0; int j = 0;
	for (c = 1; c <= a; c++)
	{
		if (a % c == 0)
		{
			printf("%d\n", c);
			int add[] = { c };
		printf("%d\n", c);
		}
	}
	for (d = 1; d <= 40; d++)
	{
		if (b % d == 0)
		{
			printf("%d\n", d);
			int  add[] = {c, d };
			printf("%d\n", c);
		}
	}
	if (c == d)
	{
		printf("%d\n", c);
	}
	printf("%d\n", h);
	system("pause");
	return 0;
}
