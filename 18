//三个数从大到小输出
int main()
{
	int a, b, c, d;
	scanf("%d%d%d", &a, &b, &c);
	if (a > b && a > c)

	{
		printf("%d>%d>%d\n", a > b ? a : b, b > c ? b : c, b > c ? c : b);
	}
	if (b > a && b > c)
	{
		printf("%d>%d>%d\n", b > a ? b : a, a > c ? a : c, a > c ? c : a);
	}
	if (c > a && c > b)
	{
		printf("%d>%d>%d\n", c > a ? c : a, a > b ? a : b, a > b ? b : a);
	}
	system("pause");
	return 0;
